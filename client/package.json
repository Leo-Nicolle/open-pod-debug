{
  "name": "openpod-debug",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-font": "fantasticon public/icon -o public/font/"
  },
  "dependencies": {
    "@creativebulma/bulma-divider": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "axios": "^0.21.4",
    "buefy": "^0.9.10",
    "core-js": "^3.6.5",
    "dom-to-image": "^2.6.0",
    "vue": "^2.6.11",
    "vue-fontawesome": "0.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "fantasticon": "^1.2.2",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.4.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-mixed-operators": 1,
      "no-param-reassign": 1,
      "no-plusplus": 1,
      "no-shadow": 1,
      "no-useless-constructor": 1,
      "no-empty-function": 1,
      "no-unused-vars": 1,
      "max-len": 1,
      "space-before-blocks" : 1,
      "space-infix-ops": 1,
      "comma-dangle": 1,
      "semi": 1,
      "no-multi-spaces": 1,
      "padded-blocks": 1,
      "indent": 1,
      "space-in-parens": 1,
      "quotes": 1,
      "operator-linebreak": 1,
      "object-curly-spacing": 1,
      "no-trailing-spaces": 1,
      "implicit-arrow-linebreak": 1,
      "arrow-parens": 1,
      "function-paren-newline": 1,
      "arrow-body-style": 1,
      "key-spacing": 1,
      "no-nested-ternary": 1
      
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
